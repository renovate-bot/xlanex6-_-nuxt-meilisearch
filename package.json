{
  "name": "nuxt-meilisearch",
  "type": "module",
  "version": "1.3.3",
  "author": "Alex Duval",
  "license": "MIT",
  "homepage": "https://nuxt-meilisearch.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xlanex6/nuxt-meilisearch.git"
  },
  "contributors": [],
  "keywords": [
    "meilisearch",
    "search",
    "vue-instantsearch",
    "nuxt",
    "module"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@meilisearch/instant-meilisearch": "0.18.1",
    "@nuxt/kit": "3.11.2",
    "instantsearch.css": "8.1.0",
    "meilisearch": "0.40.0",
    "vue-instantsearch": "4.16.2"
  },
  "devDependencies": {
    "defu": "6.1.4",
    "@nuxt/ui": "2.16.0",
    "@nuxt/devtools": "1.3.1",
    "@nuxt/eslint-config": "0.3.13",
    "@nuxt/module-builder": "0.6.0",
    "@nuxt/schema": "3.11.2",
    "@nuxt/test-utils": "3.13.1",
    "@types/node": "20.12.12",
    "changelogen": "0.5.5",
    "eslint": "9.3.0",
    "nuxt": "3.11.2",
    "typescript": "latest",
    "vitest": "1.6.0",
    "vue-tsc": "2.0.19"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "doc": "docs",
    "example": "playground"
  }
}
